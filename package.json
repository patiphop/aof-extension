{
  "name": "faizsync-extension",
  "displayName": "faizSync Extension",
  "description": "A VSCode extension for real-time file synchronization with WebSocket support",
  "version": "1.0.0",
  "publisher": "faizsync",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:faizsync.startSync",
    "onCommand:faizsync.stopSync",
    "onCommand:faizsync.openWebView"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "faizsync.startSync",
        "title": "Start faizSync",
        "category": "faizSync"
      },
      {
        "command": "faizsync.stopSync",
        "title": "Stop faizSync",
        "category": "faizSync"
      },
      {
        "command": "faizsync.openWebView",
        "title": "Open faizSync WebView",
        "category": "faizSync"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run bundle",
    "compile": "tsc -p ./",
    "bundle": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "pretest": "npm run bundle && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "npm run bundle && vsce package"
  },
  "devDependencies": {
    "@types/minimatch": "^5.1.2",
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.74.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitest/coverage-v8": "^0.34.0",
    "eslint": "^8.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.0",
    "vitest": "^0.34.0",
    "vsce": "^2.0.0",
    "webpack": "^5.101.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "minimatch": "^10.0.3",
    "ws": "^8.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patiphop/aof-extension.git"
  },
  "keywords": [
    "vscode",
    "extension",
    "sync",
    "websocket",
    "git"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/patiphop/aof-extension/issues"
  },
  "homepage": "https://github.com/patiphop/aof-extension#readme"
}
